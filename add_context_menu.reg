Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\SystemFileAssociations\.pdf\shell\PageRipJPG]
@="Extract here as JPG"

[HKEY_CLASSES_ROOT\SystemFileAssociations\.pdf\shell\PageRipJPG\command]
@="pythonw.exe \"[PATH_TO_PAGERIP.PY]\\pagerip.py\" \"%1\" jpg"

[HKEY_CLASSES_ROOT\SystemFileAssociations\.pdf\shell\PageRipPNG]
@="Extract here as PNG"

[HKEY_CLASSES_ROOT\SystemFileAssociations\.pdf\shell\PageRipPNG\command]
@="pythonw.exe \"[PATH_TO_PAGERIP.PY]\\pagerip.py\" \"%1\" png"
